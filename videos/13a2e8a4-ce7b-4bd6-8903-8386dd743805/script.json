[{"slide_number": 1, "text": "Wade Wilson: A Man of Many Masks", "image_desc": "Deadpool masked face clipart", "slide_type": "text_and_image", "image_url": "https://clipartcraft.com/images/deadpool-clipart-mask-4.png", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_1.webp", "Image Description": ""}, {"slide_number": 2, "text": "Once, Wade was a mercenary, living life on the edge.", "image_desc": "Mercenary with guns clipart", "slide_type": "text_and_image", "image_url": "https://clipground.com/images/mercenary-clipart-15.jpg", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_2.webp", "Image Description": ""}, {"slide_number": 3, "text": "But fate had a cruel twist in store for him.", "image_desc": "Cancer cell clipart", "slide_type": "text_and_image", "image_url": "https://cdn-icons-png.flaticon.com/512/9874/9874662.png", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_3.webp", "Image Description": ""}, {"slide_number": 4, "text": "Diagnosed with terminal cancer, Wade sought a cure.", "image_desc": "Doctor examining patient clipart", "slide_type": "text_and_image", "image_url": "https://image.freepik.com/free-vector/woman-doctor-examining-patient_38747-450.jpg", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_4.webp", "Image Description": ""}, {"slide_number": 5, "text": "He found it in a shadowy program called Weapon X.", "image_desc": "Weapon X logo clipart", "slide_type": "text_and_image", "image_url": "https://vignette.wikia.nocookie.net/logocomics/images/0/0f/Weapon_X_Logo.png/revision/latest?cb=20150407182508", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_5.webp", "Image Description": ""}, {"slide_number": 6, "text": "The experiment gave him superhuman abilities...", "image_desc": "Deadpool with claws clipart", "slide_type": "text_and_image", "image_url": "https://i.etsystatic.com/51085082/r/il/f7bfed/6115200656/il_570xN.6115200656_obx5.jpg", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_6.webp", "Image Description": ""}, {"slide_number": 7, "text": "...but at a terrible cost.", "image_desc": "Deadpool scarred face clipart", "slide_type": "text_and_image", "image_url": "https://www.kindpng.com/picc/m/304-3040184_head-clipart-deadpool-deadpool-svg-hd-png-download.png", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_7.webp", "Image Description": ""}, {"slide_number": 8, "text": "He became Deadpool, a twisted hero with a dark sense of humor.", "image_desc": "Deadpool breaking the fourth wall clipart", "slide_type": "text_and_image", "image_url": "http://pm1.narvii.com/5869/7bb2a2a19ecd4116766fb6a3201ed01ed4e74818_hq.jpg", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_8.webp", "Image Description": ""}, {"slide_number": 9, "text": "Despite his flaws, Deadpool fights for what he believes in.", "image_desc": "Deadpool fighting villain clipart", "slide_type": "text_and_image", "image_url": "https://www.kindpng.com/picc/m/55-558904_deadpool-attack-clip-arts-deadpool-png-transparent-png.png", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_9.webp", "Image Description": ""}, {"slide_number": 10, "text": "He's a reminder that even in the darkest of times, hope can be found.", "image_desc": "Deadpool smiling clipart", "slide_type": "text_and_image", "image_url": "https://icon-library.com/images/kisspng-amazon-echo-amazon-alexa-google-play-smiley-deadpool-icon-5b361a6cb04104.559179361530272364722.jpg", "image_path": "data/videos/13a2e8a4-ce7b-4bd6-8903-8386dd743805/images/image_10.webp", "Image Description": ""}]